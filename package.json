{
  "name": "dimondchat",
  "version": "1.0.0",
  "description": "Chat App with Node.js Vue.js",
  "main": "server/main/app.ts",
  "scripts": {
    "start": "node server/dist/main/index.js",
    "dev": "npm run build:live",
    "build": "node_modules/.bin/tsc -p .",
    "build:live": "node_modules/.bin/nodemon --watch '*.ts' --exec 'ts-node' server/main/index.ts",
    "tsc": "node_modules/.bin/tsc",
    "heroku-prebuild": "npm install npm@latest -g && cd client && npm install && npm install --only=dev",
    "heroku-postbuild": "npm run build && cd client && npm run build"
  },
  "author": "Onur Iltan",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.1.22",
    "@types/mongoose": "^5.3.22",
    "@types/morgan": "^1.7.35",
    "@types/query-string": "^6.3.0",
    "@types/request": "^2.48.1",
    "@types/socket.io": "^2.1.2",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/node": "^11.13.8",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3"
  }
}
