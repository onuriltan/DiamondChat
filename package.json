{
  "name": "dimondchat",
  "version": "1.0.0",
  "description": "Chat App with Node.js Vue.js",
  "main": "server/main/app.ts",
  "scripts": {
    "start": "node server/dist/main/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "npm run build:live",
    "client": "npm run serve --prefix client",
    "build": "node_modules/.bin/tsc -p .",
    "build:live": "node_modules/.bin/nodemon --watch '*.ts' --exec 'ts-node' server/main/index.ts",
    "tsc": "node_modules/.bin/tsc",
    "heroku-prebuild": "npm install npm@latest -g && cd client && npm install && npm install --only=dev",
    "heroku-postbuild": "npm run build && cd client && npm run build"
  },
  "author": "Onur Iltan",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongodb": "^3.3.6",
    "@types/mongoose": "^5.5.22",
    "@types/morgan": "^1.7.37",
    "@types/query-string": "^6.3.0",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.4",
    "axios": "^0.18.1",
    "bcrypt": "^3.0.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/node": "^11.13.22",
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1"
  }
}
